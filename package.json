{
  "name": "blewett-directory",
  "version": "1.0.0",
  "description": "A community directory of small businesses in Blewett, BC, Canada",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch src/scss:src/static/css",
    "build:sass": "sass src/scss:src/static/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "author": "Agata Zurek",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "markdown-it": "^12.3.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10"
  }
}
